db <- function (inputs) {
output <- c()
for (input in inputs) {
if (input %% 4 == 0) {
number = input / 4
output <- c(output, number)
} else if (input %% 2 == 0) {
number = input / 2
output <- c(output, number)
} else {
output <- c(output, input)
}
}
return(output)
}
db(c(1, 3, 6, 8, 7))
