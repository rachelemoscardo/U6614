#Let's make it a function
num_of_vowels <- function(input) {
split <- unlist(strsplit(input, ""))
return(sum(split %in% c("a", "e", "i", "o", "u")))
}
#Check. How many vowels are there in the alphabet
num_of_vowels("abcdefghijklmnopqrstuvwxyz")
